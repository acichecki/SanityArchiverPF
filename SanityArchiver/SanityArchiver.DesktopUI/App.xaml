<Application x:Class="SanityArchiver.DesktopUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SanityArchiver.DesktopUI"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>

            <ResourceDictionary x:Key="ResourceDictionary">
                <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary Source="/Views/resources/File.xaml"/>
                    <ResourceDictionary Source="/Views/resources/Folder.xaml"/>
                    <ResourceDictionary Source="/Views/resources/Drive.xaml"/>
                </ResourceDictionary.MergedDictionaries>  
            </ResourceDictionary>

    </Application.Resources>
</Application>
